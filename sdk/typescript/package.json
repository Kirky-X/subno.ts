{
  "name": "securenotify-sdk",
  "version": "1.0.0",
  "description": "SecureNotify TypeScript SDK for encrypted push notification services",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts"
  },
  "keywords": [
    "securenotify",
    "push-notification",
    "encrypted",
    "sse",
    "real-time"
  ],
  "author": "Kirky.X",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/subno.ts"
  },
  "bugs": {
    "url": "https://github.com/your-org/subno.ts/issues"
  },
  "homepage": "https://securenotify.dev",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
